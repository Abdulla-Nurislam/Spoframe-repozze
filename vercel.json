{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/$",
      "dest": "/public/Landing Page.html"
    },
    {
      "src": "/catalog",
      "dest": "/public/Product Catalog.html"
    },
    {
      "src": "/product",
      "dest": "/public/Product Detail.html"
    },
    {
      "src": "/about",
      "dest": "/public/About Product.html"
    },
    {
      "src": "/register",
      "dest": "/public/register.html"
    },
    {
      "src": "/admin",
      "dest": "/public/admin.html"
    },
    {
      "src": "/profile",
      "dest": "/public/profile.html"
    },
    {
      "src": "/user-feedback",
      "dest": "/public/user-feedback.html"
    },
    {
      "src": "/development-page",
      "dest": "/public/development-page.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 